{
  "name": "@semibold/fullscreen",
  "version": "3.0.0",
  "description": "JavaScript Fullscreen API. Support different frames(documents)",
  "browser": "dist/release/fullscreen.js",
  "types": "src/index.ts",
  "scripts": {
    "build": "webpack --mode=production --devtool=source-map",
    "start": "webpack -w --mode=development --devtool=cheap-module-eval-source-map",
    "tslint": "tslint -p ./ \"./**/*.ts\" \"./**/*.tsx\"",
    "version": "npm run build && git add -A dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Semibold/Fullscreen.git"
  },
  "keywords": [
    "browser",
    "fullscreen"
  ],
  "author": "Aqours",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Semibold/Fullscreen/issues"
  },
  "homepage": "https://github.com/Semibold/Fullscreen#readme",
  "engines": {
    "node": ">=8.9.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged",
      "pre-push": "npm run tslint"
    }
  },
  "devDependencies": {
    "@types/node": "10.9.4",
    "event-hooks-webpack-plugin": "2.1.1",
    "git-rev-sync": "1.12.0",
    "husky": "1.1.4",
    "prettier": "1.15.2",
    "pretty-quick": "1.8.0",
    "rimraf": "2.6.2",
    "ts-loader": "5.3.0",
    "tslint": "5.11.0",
    "tslint-config-prettier": "1.16.0",
    "tslint-plugin-prettier": "2.0.1",
    "typescript": "3.1.6",
    "uglifyjs-webpack-plugin": "2.0.1",
    "webpack": "4.25.1",
    "webpack-bundle-analyzer": "3.0.3",
    "webpack-cli": "3.1.2"
  }
}
