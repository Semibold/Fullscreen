{
  "name": "@semibold/fullscreen",
  "version": "2.0.3",
  "description": "JavaScript Fullscreen API. Support different frames(documents)",
  "main": "dist/release/fullscreen.js",
  "types": "src/index.ts",
  "scripts": {
    "build": "npm run tslint && webpack --display=errors-only --mode=production",
    "start": "webpack -w --mode=development",
    "tslint": "tslint -p ./ ./**/*.ts ./**/*.tsx",
    "pretty": "prettier --write ./{webpack.config.js,{src,test,types}/**/*.{js,jsx,jsm,ts,tsx,css,less,json,md}}",
    "version": "npm run build && git add -A dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Semibold/Fullscreen.git"
  },
  "keywords": [
    "fullscreen",
    "cross-browser",
    "javascript"
  ],
  "author": "Aqours",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Semibold/Fullscreen/issues"
  },
  "homepage": "https://github.com/Semibold/Fullscreen#readme",
  "engines": {
    "node": ">=8.9.0"
  },
  "prettier": {
    "printWidth": 120
  },
  "devDependencies": {
    "@types/node": "10.9.4",
    "event-hooks-webpack-plugin": "2.1.0",
    "git-rev-sync": "1.12.0",
    "prettier": "1.14.2",
    "rimraf": "2.6.2",
    "ts-loader": "5.0.0",
    "ts-node": "7.0.1",
    "tslint": "5.11.0",
    "typescript": "3.0.3",
    "webpack": "4.17.2",
    "webpack-cli": "3.1.0"
  }
}
